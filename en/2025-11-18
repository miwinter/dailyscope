<script>
    anychart.onDocumentReady(function() {
        var map = anychart.map();
        map.geoData('anychart.maps.world');
        map.interactivity().selectionMode('none');
        
        // Configurer les régions non couvertes
        map.unboundRegions()
            .enabled(true)
            .fill('#f0f0f0')
            .stroke({
                color: '#000000',
                thickness: 0.5,
                lineJoin: 'round'
            });
        map.padding([0, 0, 0, 0]);
            
          // Construire les données des séries dynamiquement à partir des clusters
        var seriesData = [
        
            {
                name: "France-Ukraine Rafale Jets and Defense Systems Agreement",
                color: "#72e5ac",
                data: [ {"id": "AE"},  {"id": "AL"},  {"id": "AR"},  {"id": "AT"},  {"id": "AZ"},  {"id": "BE"},  {"id": "BH"},  {"id": "BJ"},  {"id": "BR"},  {"id": "CL"},  {"id": "CO"},  {"id": "CZ"},  {"id": "EE"},  {"id": "EG"},  {"id": "ES"},  {"id": "GB"},  {"id": "GR"},  {"id": "HU"},  {"id": "IN"},  {"id": "IS"},  {"id": "IT"},  {"id": "JP"},  {"id": "KR"},  {"id": "LU"},  {"id": "NP"},  {"id": "OM"},  {"id": "PH"},  {"id": "PT"},  {"id": "SA"},  {"id": "SE"},  {"id": "SK"},  {"id": "US"},  {"id": "VN"} ]
            },
        
            {
                name: "French Political and Diplomatic Context",
                color: "#72a2e5",
                data: [ {"id": "BF"},  {"id": "PG"} ]
            },
        
            {
                name: "International and UN Security Council Developments",
                color: "#bf72e5",
                data: [ {"id": "GU"} ]
            },
        
            {
                name: "Other International News Involving France",
                color: "#e5728f",
                data: [ {"id": "HT"},  {"id": "TD"} ]
            },
        
            {
                name: "Global Aid and Francophonie Initiatives",
                color: "#e5d272",
                data: [ {"id": "RW"} ]
            }
        
        
            ,
            {
                name: "Different perspectives:\n- France-Ukraine Rafale Jets and Defense Systems Agreement\n- International and UN Security Council Developments",
                color: "#AAAAAA",
                data: [{"id": "MN"}]
            },
            {
                name: "Different perspectives:\n- France-Ukraine Rafale Jets and Defense Systems Agreement\n- International and UN Security Council Developments",
                color: "#AAAAAA",
                data: [{"id": "MR"}]
            },
            {
                name: "Different perspectives:\n- France-Ukraine Rafale Jets and Defense Systems Agreement\n- Global Aid and Francophonie Initiatives",
                color: "#AAAAAA",
                data: [{"id": "MX"}]
            },
            {
                name: "Different perspectives:\n- France-Ukraine Rafale Jets and Defense Systems Agreement\n- French Political and Diplomatic Context",
                color: "#AAAAAA",
                data: [{"id": "RU"}]
            },
            {
                name: "Different perspectives:\n- France-Ukraine Rafale Jets and Defense Systems Agreement\n- French Political and Diplomatic Context",
                color: "#AAAAAA",
                data: [{"id": "XK"}]
            }
        
        ];
        
        // Créer une série pour chaque cluster
        seriesData.forEach(function(seriesInfo) {
            var series = map.choropleth(seriesInfo.data);
            series.name(seriesInfo.name);
            series.geoIdField('id');
            series.fill(seriesInfo.color);
            series.stroke('#000000', 0.5, 'round');
            
            // Configuration du hover
            series.hovered()
                .fill(anychart.color.lighten(seriesInfo.color, 0.2));
            
            // Configuration des tooltips
            series.tooltip().format(function() {
                // this.series.name() donne le titre du cluster, this.getData('name') le code pays
                return this.series.name()
            });
        });
        
        // Désactiver la légende
        map.legend(false);
        
        // Rendre la carte
        map.container('map-container');
        map.draw();
    });

    function toggleSources(button) {
        var content = button.nextElementSibling;
        var isHidden = content.classList.contains('hidden');
        
        content.classList.toggle('hidden');
        button.innerHTML = isHidden ? 'Hide sources ▲' : 'Show sources ▼';
    }
</script>

